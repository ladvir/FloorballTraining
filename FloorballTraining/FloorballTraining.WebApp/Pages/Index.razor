@page "/"
@using FloorballTraining.UseCases.PluginInterfaces
@using FloorballTraining.CoreBusiness
@inject IActivityRepository ViewActivitiesByNameUseCase


<RangeSlider MaxValue="50" MinValue="1" ValueHigh="40" ValueLow="25" Color="Color.Success"></RangeSlider>
@code
{
    private List<Activity> _activities = new List<Activity>();

    protected override async Task OnInitializedAsync()
    {
        _activities = (await ViewActivitiesByNameUseCase.GetActivitiesByNameAsync()).ToList();
    }


}
